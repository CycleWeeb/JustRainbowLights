<vertical child-control-height="false" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://monkeymanboy.github.io/BSML-Docs/ https://raw.githubusercontent.com/monkeymanboy/BSML-Docs/gh-pages/BSMLSchema.xsd">
  <horizontal bg="panel-top" pad-left="10" pad-right="10" horizontal-fit="PreferredSize">
    <text text="Preset Description" align="Center" font-size="10" />
  </horizontal>
  <horizontal preferred-width="100" preferred-height="45">
    <text-page id="rainbow-description" />
  </horizontal>
  <bool-setting text="Enable" hover-hint="Enable JustRainbowLights" value="enable" apply-on-change="true" font-size="4" />
  <clickable-text text="Report Bugs Here!" color="blue" highlight-color="lightblue" on-click="reportBugs" />
</vertical>